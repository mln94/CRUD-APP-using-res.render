<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
<a href="/add">Add user</a>
<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Phone Number</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        <% if (locals.message) { %>
            <p><%= message %></p>
        <% } %>
        <% users.forEach(user => { %>
            <tr>
                <td><%= user.firstName %></td>
                <td><%= user.lastName %></td>
                <td><%= user.email %></td>
                <td><%= user.phoneNumber %></td>
                <td>
                    <a href="/update/<%= user.id %>">Update</a>
                    <a class="delete" data-id="<%= user.id %>">Delete</a>
                </td>
         <% }) %>
    </tbody>
</table>
<script src="/js/script.js"> </script> 
</body>
</html>